---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import Headers from '../components/widget/Header.astro';
import Intro from '../components/widget/Intro.astro';
import Fastfood from '../components/widget/Fastfood.astro';
import Resto from '../components/widget/Resto.astro';
import Icon from '../components/widget/Icon.astro';
import Android from '../components/widget/Android.astro';
import Bebas from '../components/widget/Freedom.astro';
import { SITE_TITLE, SITE_DESCRIPTION, } from '../config';
---

<!DOCTYPE html>
<html lang="id">
<head>
<meta name="google-site-verification" content="Xi3WpckDbMBvm1pdlc5XukNoA9uWDmnExIiOeaTOxq0" />
<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
</head>
<body>
<div class="container">
<Header title={SITE_TITLE} />
<main class="row">
<Headers/>
<Intro/>
<Icon/>
<Android/>
<Bebas/>
</main>
<Footer />
</div>
</body>
</html>
