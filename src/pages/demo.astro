---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import Panel from '../components/widget/demo/Panel.astro';
import Content from '../components/widget/demo/Content.astro';
import { SITE_TITLE, SITE_DESCRIPTION, } from '../config';
import {title,description,play } from '../data/demo';
---
<!DOCTYPE html>
<html lang="id">
<head>
<meta name="google-site-verification" content="Xi3WpckDbMBvm1pdlc5XukNoA9uWDmnExIiOeaTOxq0" />
<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
</head>
<body>
<div class="container">
<Header title={SITE_TITLE} />
<main class="row">
<div class="col-md-12 p-3 p-md-5">
<h1><strong><a href="/demo/">{title}</a></strong></h1>
<h2>{description}</h2>
<div class="row">
<p>{play}</p>
<Panel/>
<Content/>
</div>
</main>
<Footer />
</div>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
</body>
</html>